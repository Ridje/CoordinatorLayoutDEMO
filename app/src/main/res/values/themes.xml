<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CoordinatorLayoutDEMO" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FFD600</item>
        <item name="colorPrimaryVariant">#000000</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#131313</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#FFD600</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.CoordinatorLayoutDEMO.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.CoordinatorLayoutDEMO.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.CoordinatorLayoutDEMO.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
